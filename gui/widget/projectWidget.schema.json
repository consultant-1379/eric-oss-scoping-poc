{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "projectWidget.schema.json",
  "title": "Project widget",
  "description": "Project widget",
  "type": "object",
  "required": [
    "name"
  ],
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "type": {
      "enum": [
        "Table",
        "HTML",
        "LineChart",
        "Donut",
        "Gauge",
        "Form"
      ]
    },
    "config": {
      "type": "object"
    }
  }
}