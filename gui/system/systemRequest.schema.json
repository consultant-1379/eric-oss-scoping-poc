{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "systemRequest.schema.json",
  "title": "System Request",
  "description": "Collect additional request information like cookie and headers into arg of automation.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "cookies": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "headers": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    }
  }
}